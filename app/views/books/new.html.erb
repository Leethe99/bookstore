<h1>new book</h1>
<%= form_with model: @book do |form|%>
    <div>
    <%= form.label :title%>
    <%= form.text_field :title%>

    <%= form.label :author%>
    <%= form.text_field :author%>

    <%= form.label :year%>
    <%= form.number_field :year%>
    </div>
    <div>
     <%= form.label :description%>
    <%= form.text_area  :year%>
    </div>
    <%= form.button "add book"  %>

<%end%>